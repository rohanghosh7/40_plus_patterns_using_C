#include<stdio.h>
int main()
{
 int i,j;
 clrscr();
 for(i=1;i<=5;i++)
 {
  for(j=1;j<=9;j++)
  {
   if(j>=6-i && j<=11-(2*i))
    printf("*");
   else
    printf(" ");
   }
  printf("\n");
 }
 for(i=1;i<=4;i++)
 {
  for(j=1;j<=8;j++)
  {
   if(j>=1+i && j<=(1+2*i))
    printf("*");
   else
    printf(" ");
   }
  printf("\n");
 }
return 0;
}
